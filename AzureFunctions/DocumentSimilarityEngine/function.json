{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    { 
      "type": "blob",
      "direction": "in",
      "name": "pickledmodel",
      "path": "model/test.pkl",
      "connection": "AzureWebJobsStorage",
      "dataType": "binary"
    }
  ]
}
